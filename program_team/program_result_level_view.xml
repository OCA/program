<?xml version="1.0" encoding="utf-8" ?>
<openerp>
  <data>

    <record id="view_program_result_level_form" model="ir.ui.view">
      <field name="name">Program Result Level Form</field>
      <field name="model">program.result.level</field>
      <field name="inherit_id" ref="program.view_program_result_level_form"/>
      <field name="arch" type="xml">

        <field name="fvg_show_field_statement" position="after">

          <field name="fvg_show_field_department_id"/>

        </field>

        <group name="fvg_show">

          <field name="fvg_show_page_team"/>
          <field name="fvg_show_page_beneficiary"/>
          <field name="fvg_show_page_partner"/>

        </group>

      </field>
    </record>

  </data>
</openerp>
